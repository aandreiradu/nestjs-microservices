{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/credit-bureau/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/credit-bureau/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/credit-bureau",
  "projects": {
    "credit-bureau": {
      "type": "application",
      "root": "apps/credit-bureau",
      "entryFile": "main",
      "sourceRoot": "apps/credit-bureau/src",
      "compilerOptions": {
        "tsConfigPath": "apps/credit-bureau/tsconfig.app.json"
      }
    },
    "bank-a": {
      "type": "application",
      "root": "apps/bank-a",
      "entryFile": "main",
      "sourceRoot": "apps/bank-a/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank-a/tsconfig.app.json"
      }
    },
    "bank-b": {
      "type": "application",
      "root": "apps/bank-b",
      "entryFile": "main",
      "sourceRoot": "apps/bank-b/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank-b/tsconfig.app.json"
      }
    },
    "bank-c": {
      "type": "application",
      "root": "apps/bank-c",
      "entryFile": "main",
      "sourceRoot": "apps/bank-c/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank-c/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}